{
  "name": "pekral/rector-rules",
  "description": "Rector - custom rule set",
  "license": ["MIT"],
  "keywords": [
    "rector",
    "ruleset",
    "php",
    "refactoring",
    "coding-standard",
    "pekral"
  ],
  "authors": [
    {
      "name": "Petr Kr√°l",
      "email": "kral.petr.88@gmail.com",
      "role": "lead"
    }
  ],
  "require": {
    "php": "^8.4",
    "rector/rector": "^2.1.2"
  }
  ,
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "PekralRectorRule\\": "rules"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "post-update-cmd": [
      "vendor/bin/rector process rules/"
    ]
  },
  "type": "library",
  "homepage": "https://github.com/pekral/rector-rules",
  "support": {
    "issues": "https://github.com/pekral/rector-rules/issues",
    "source": "https://github.com/pekral/rector-rules"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/pekral"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "PekralRectorRule\\Tests\\": "tests/"
    }
  }
}
